{
    "name": "extracellular_af_qc_cluster",
    "data_path": "data/bm_update_3_subset_lig_features_coexpression_lig_type_clusters.csv",
    "filters": {
        "column_equals": {
            "lig1_location": "E"
        },
        "non_nan_columns": [
            "spoc_score"
        ]
    },
    "feature_groups": {
        "residue_contacts": true,
        "distance_metrics": true,
        "ligand_contact_sum": true,
        "ligand_contact_indiv": true,
        "alphafold_metrics": true,
        "expression_features": true,
        "ligand_metadata": true,
        "receptor_metadata": false,
        "spoc_metrics": false,
        "expression_profiles": false
    },
    "model_params": {
        "d_model": 256,
        "n_heads": 8,
        "n_layers": 4,
        "d_ff": 1024,
        "dropout": 0.1,
        "batch_size": 32,
        "learning_rate": 1e-4,
        "weight_decay": 0.01,
        "warmup_steps": 1000,
        "max_epochs": 100,
        "early_stopping_patience": 10
    },
    "data_params": {
        "test_size": 0.1,
        "valid_size": 0.1,
        "random_state": 42,
        "num_workers": 4,
        "split_method": "cluster"
    },
    "cluster_column": "nmfUMAP_af_qc_clusters"
} 